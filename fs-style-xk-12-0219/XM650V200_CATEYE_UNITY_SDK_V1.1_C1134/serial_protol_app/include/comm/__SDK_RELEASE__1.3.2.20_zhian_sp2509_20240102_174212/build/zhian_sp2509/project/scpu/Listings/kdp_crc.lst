Nstruct SHA1Context sha_core;
N
Nu32 kdp_gen_sha32(u8 *data, u32 size)
N{
N    u32 *sram, *ddr, i, j;
N    ddr = (u32*)data;
N    sram = (u32*)(sha_core.Message_Block);
N    SHA1Reset(&sha_core);
N    for (i = 0; i < size; i += 64)
N    {
N#if 0
S        memcpy(sram, ddr, 64);
S        ddr += 16;
N#else
N        for (j = 0; j < 16; j++)  // copy one block over
N            sram[j] = *ddr++;
N#endif
N        SHA1ProcessMessageBlock(&sha_core);
N    }
N    if (size & 0x03F)  // pad partial block with zero's
N    {
N        i -= 64; // point to the start of partial block
N        memset(sha_core.Message_Block, 0, 64);
N        memcpy(sha_core.Message_Block, &data[i], size & 0x03F);
N        SHA1ProcessMessageBlock(&sha_core);
N    }
N    j = 0;
N    for (i = 0; i < 5; i++)
N        j ^= sha_core.Intermediate_Hash[i];
N    return(j);
N}
