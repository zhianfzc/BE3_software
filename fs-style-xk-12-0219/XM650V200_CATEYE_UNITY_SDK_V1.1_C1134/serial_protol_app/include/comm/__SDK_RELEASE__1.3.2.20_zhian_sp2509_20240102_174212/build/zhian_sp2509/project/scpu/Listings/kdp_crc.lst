Nint SHA1Reset(SHA1Context *context)
N{
N    if (!context)
N    {
N        return shaNull;
N    }
N
N    context->Length_Low             = 0;
N    context->Length_High            = 0;
N    context->Message_Block_Index    = 0;
N
N    context->Intermediate_Hash[0]   = 0x67452301;
N    context->Intermediate_Hash[1]   = 0xEFCDAB89;
N    context->Intermediate_Hash[2]   = 0x98BADCFE;
N    context->Intermediate_Hash[3]   = 0x10325476;
N    context->Intermediate_Hash[4]   = 0xC3D2E1F0;
N
N    context->Computed   = 0;
N    context->Corrupted  = 0;
N
N    return shaSuccess;
N}
