Nu16 kdp_gen_crc16(u8 *data, u32 size)
N{
N    u16 out = 0;
N    int bits_read = 0, bit_flag, i;
N
N    /* Sanity check: */
N    if (data == NULL)
X    if (data == 0)
N        return 0;
N
N    while (size > 0)
N    {
N        bit_flag = out >> 15;
N
N        /* Get next bit: */
N        out <<= 1;
N        out |= (*data >> bits_read) & 1;
N        bits_read++;
N        if (bits_read > 7)
N        {
N            bits_read = 0;
N            data++;
N            size--;
N        }
N
N        /* Cycle check: */
N        if (bit_flag)
N            out ^= CRC16_CONSTANT;
X            out ^= 0x8005;
N
N    }
N
N    // push out the last 16 bits
N    for (i = 0; i < 16; ++i) {
N        bit_flag = out >> 15;
N        out <<= 1;
N        if (bit_flag)
N            out ^= CRC16_CONSTANT;
X            out ^= 0x8005;
N    }
N
N    // reverse the bits
N    u16 crc = 0;
N    i = 0x8000;
N    int j = 0x0001;
N    for (; i != 0; i >>= 1, j <<= 1) {
N        if (i & out) crc |= j;
N    }
N
N    return crc;
N}
