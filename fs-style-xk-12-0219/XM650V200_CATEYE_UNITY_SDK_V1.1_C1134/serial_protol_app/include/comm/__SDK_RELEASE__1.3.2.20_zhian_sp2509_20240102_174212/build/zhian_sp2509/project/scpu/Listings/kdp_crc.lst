N/* Calculate the 32-bit checksum of object using 32-bit size block
N** If address is not 32-bit aligned, use the fraction of the block w/ zero's
N** replacing excluded bytes.  (Note: we use Little Endian)
N*/
Nu32 kdp_gen_sum32(u8 *data, u32 size)
N{
N    u32 sum, *ddr, i = (u32)data & 0x03;
N    ddr = (u32*) ((u32)data & 0xFFFFFFFC);  // point to the first 32-bit aligned block
N    if (i) { // starting address misaligned ?
N		size = size + i - 4;
N        sum = *ddr;
N        sum >>= i*8;
N		sum <<= i*8;
N        ddr++;
N    }
N    else
N        sum = 0;
N    for (i = 0; i < (size & 0xFFFFFFFC); i += 4) {
N        sum += *ddr;
N        ddr++;
N    }
N    i = size & 3;
N    if (i)  // ending address misaligned ?
N    {
N        size = *ddr;
N        size <<= (4-i)*8;
N        size >>= (4-i)*8;
N        sum += size;
N    }
N    return(sum);
N}
